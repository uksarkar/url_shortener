type Query {
  # Link
  links(pagination: PaginationQuery!, sort: SortBy): LinksResult!
  getLink(id: Int!): Link!

  # Domain
  domains(pagination: PaginationQuery!, sort: SortBy): DomainsResult!
  getDomain(id: Int!): Domain!

  # User
  users(pagination: PaginationQuery!, sort: SortBy): UserResult!
  getUser(id: Int!): User!
}
