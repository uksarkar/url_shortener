# Models
type Link {
  id: Int!
  original_Link: String!
  hash: String!
  domain_id: Int
  is_active: Boolean!
  created_at: String!
  updated_at: String!
}

type LinksResult {
  data: [Link!]!
  meta: PaginationMeta!
}


# Form inputs
input CreateLink {
  original_Link: String!
  is_active: Boolean!
  hash: String
  domain_id: Int
}